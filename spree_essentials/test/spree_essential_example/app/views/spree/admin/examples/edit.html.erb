<% if @example.try(:errors).present? %>
  <%= render 'spree/shared/error_messages', :target => @example %>
<% end %>

<h1>Edit Example</h1>

<%= form_for([:admin, @example]) do |f| %>
  <%= render "form", :form => f %>
  <p class="form-buttons">
    <%= button t("update") %> <%= t('or') %> <%= link_to t('cancel'), collection_url %>
  </p>
<% end %>
